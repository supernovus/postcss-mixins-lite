{
  "name": "postcss-mixins-lite",
  "version": "12.0.0",
  "description": "PostCSS plugin for mixins (lite fork)",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "mixins",
    "sass"
  ],
  "scripts": {
    "unit": "node --test test/index.test.js",
    "test:coverage": "c8 pnpm unit",
    "test:lint": "eslint .",
    "test": "pnpm run /^test:/"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "supernovus/postcss-mixins-lite",
  "engines": {
    "node": "^18.0 || ^ 20.0 || >= 22.0"
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/postcss/"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/ai"
    }
  ],
  "peerDependencies": {
    "postcss": "^8.2.14"
  },
  "dependencies": {
    "postcss-js": "^4.0.1",
    "postcss-simple-vars": "^7.0.1"
  },
  "devDependencies": {
    "@logux/eslint-config": "^53.4.0",
    "c8": "^10.1.2",
    "clean-publish": "^5.0.0",
    "eslint": "^9.11.1",
    "postcss": "^8.4.47"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "c8": {
    "exclude": [
      "**/*.test.*"
    ],
    "lines": 100,
    "reporter": "lcov",
    "check-coverage": true
  },
  "clean-publish": {
    "cleanDocs": true
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
